{
  "name": "cesnet-oozie",
  "version": "1.0.0",
  "author": "Frantisek Dvorak <valtri@civ.zcu.cz>",
  "summary": "Apache Oozie Workflow Scheduler",
  "license": "MIT",
  "source": "https://github.com/MetaCenterCloudPuppet/cesnet-oozie",
  "project_page": "https://github.com/MetaCenterCloudPuppet/cesnet-oozie",
  "issues_url": "https://github.com/MetaCenterCloudPuppet/cesnet-oozie/issues",
  "tags": ["hadoop", "oozie", "workflow"],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": ["6", "7"]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": ["7", "8", "9", "10"]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["6", "7"]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": ["6", "7"]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["14.04", "16.04", "18.04"]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.4.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 1.0.0 <7.0.0"
    },
    {
      "name": "adrien/alternatives",
      "version_requirement": ">= 0.3.0 <1.0.0"
    },
    {
      "name": "cesnet/hadoop",
      "version_requirement": ">= 3.0.0 <4.0.0"
    },
    {
      "name": "cesnet/hadoop_lib",
      "version_requirement": ">=0.4.0 <1.0.0"
    }
  ]
}
